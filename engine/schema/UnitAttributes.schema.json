{
  "$schema": "http://json-schema.org/schema#",
  "$id": "http://martykane.org/schemas/UnitAttributes.schema.json",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "movement": {
      "type": "integer",
      "min": 1
    },
    "turnOrder": {
      "type": "integer",
      "min": 0,
      "max": 99
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "movement"
  ]
}
